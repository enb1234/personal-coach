<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.brunokam.personalcoach" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ResolutionListActivity"
            android:label="@string/title_activity_resolution_list" >
        </activity>
        <activity
            android:name=".ResolutionDetails"
            android:label="@string/title_activity_resolution_details" >
        </activity>
    </application>

</manifest>
